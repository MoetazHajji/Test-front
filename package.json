{
    "name": "back-api",
    "version": "1.6.0",
    "main": "./build/index.js",
    "keywords": [
        "yeoman-generator",
        "typescript",
        "node",
        "nodejs",
        "rest",
        "restfull",
        "api",
        "builder",
        "generator",
        "swagger",
        "openapi",
        "jsdoc"
    ],
    "description": "back of services project",
    "scripts": {
        "version": "cat package.json | grep 'version' | cut -d ':' -f 2 | sed 's/^..//' | sed 's/..$//'",
        "build": "rm -rf node_modules/mongodb && ./node_modules/.bin/tsc  --skipLibCheck ",
        "start": "./node_modules/.bin/tsc --skipLibCheck &&  nodemon ./build/config/server/index.js && export PAYENV=\"sandbox\" && export EMAIL_TO=\"moetaz.hajji@allence-tunisie.com\" ",
        "ts:watch": "../node_modules/.bin/tsc -w",
        "swagger--": "node_modules/swagger-jsdoc/bin/swagger-jsdoc.js  --definition swaggerDef.js",
        "swagger": "rm -rf node_modules/swagger-jsdoc && git clone https://github.com/Surnet/swagger-jsdoc.git node_modules/swagger-jsdoc && node_modules/swagger-jsdoc/bin/swagger-jsdoc.js  --definition swaggerDef.js ",
        "clean": "rm -rf build target *-release-*.zip .angular node_modules",
        "prune": "curl -sf https://gobinaries.com/tj/node-prune | sh && node-prune ./node_modules",
        "package": "npm run clean && npm install && npm run build && npm run zip",
        "zip": "cd build && npm run prune && zip -x '*.git*' -r ../$npm_package_name-release-$npm_package_version.zip ./",
        "release": "release-it"
    },
    "files": [
        "api/src"
    ],
    "private": true,
    "release-it": {
        "gitlab": {
            "release": true
        }
    },
    "dependencies": {
        "@types/nodemailer": "^6.4.4",
        "archiver": "^5.3.1",
        "aws-sdk": "^2.1093.0",
        "axios": "^0.21.1",
        "axios-retry": "^3.3.1",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "cookie-parser": "1.4.5",
        "cors": "2.8.5",
        "dotenv": "^16.0.3",
        "express": "4.17.1",
        "helmet": "3.22.0",
        "hummus": "1.0.112",
        "imap": "^0.8.19",
        "joi": "^14.3.1",
        "js-sha256": "^0.9.0",
        "mailparser": "^3.6.3",
        "modclean": "^3.0.0-beta.1",
        "mongodb": "^5.5.0",
        "mongoose": "5.9.13",
        "nodemailer": "^6.7.7",
        "pusher": "^5.0.1",
        "serverless-http": "^2.5.0",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "4.0.2",
        "util": "0.12.3",
        "uuidv4": "^6.2.13"
    },
    "devDependencies": {
        "@types/bcrypt": "3.0.0",
        "@types/body-parser": "1.16.8",
        "@types/chai": "4.1.7",
        "@types/compression": "0.0.36",
        "@types/connect-mongo": "0.0.40",
        "@types/cookie-parser": "1.4.1",
        "@types/cors": "2.8.3",
        "@types/debug": "0.0.31",
        "@types/dotenv": "6.1.0",
        "@types/ejs": "2.6.1",
        "@types/express": "4.11.0",
        "@types/express-session": "1.15.12",
        "@types/helmet": "0.0.42",
        "@types/joi": "14.0.1",
        "@types/mongoose": "5.3.5",
        "@types/node": "^18.7.14",
        "@types/passport": "1.0.0",
        "@types/passport-local": "1.0.33",
        "@types/supertest": "2.0.7",
        "@types/swagger-jsdoc": "0.0.2",
        "@types/swagger-ui-express": "3.0.0",
        "@types/yeoman-generator": "3.1.0",
        "chai": "4.2.0",
        "jsdoc": "^3.6.6",
        "nodemon": "^2.0.19",
        "nyc": "^15.1.0",
        "source-map-loader": "0.2.4",
        "supertest": "4.0.2",
        "swagger-jsdoc": "^6.1.0",
        "swagger-ui-express": "4.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2"
    }
}
